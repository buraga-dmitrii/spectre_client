<div class="page-header">
  <h1>Logins</h1>
</div>
<div class="container">
  <h2>Updating data information...</h2>
  <p>We are now refreshing data for logins, accounts and transactions. Please wait a moment, this process may take up to 3 minutes.</p> 
  <div class="progress">
    <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:100%">
    </div>
  </div>
</div>
<script>
  fetch('/logins?refresh=true', {
      credentials: 'include',
      method: 'GET',
      headers: {
        'Accept': 'application/json',
        'Content-Type': 'application/json'
      }
    }).then((res) => {
      return res.json()
    }).then(res => {
      window.location.replace("/logins");
    })  
</script>